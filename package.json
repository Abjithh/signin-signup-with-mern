    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "dev": "nodemon --exec npm run restart",
        "restart": "rimraf dist && npm run build && npm run start",
        "challenge-server": "nodemon --exec npm run challenge-restart",
        "challenge-restart": "rimraf dist && npm run challenge-build && npm run challenge-start",
        "challenge-build": "babel src --out-dir dist --copy-files",
        "challenge-start": "node dist/challenge-server.js",
        "battle-server": "nodemon --exec npm run battle-restart",
        "battle-restart": "rimraf dist && npm run battle-build && npm run battle-start",
        "battle-build": "babel src --out-dir dist --copy-files",
        "battle-start": "node dist/battle-server.js",
        "activity-server": "nodemon --exec npm run activity-restart",
        "activity-restart": "rimraf dist && npm run activity-build && npm run activity-start",
        "activity-build": "babel src --out-dir dist --copy-files",
        "activity-start": "node dist/activity-server.js",
        "search-server": "nodemon --exec npm run search-restart",
        "search-restart": "rimraf dist && npm run search-build && npm run search-start",
        "search-build": "babel src --out-dir dist --copy-files",
        "search-start": "node dist/search-server.js"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
